<Window x:Class="MashAttack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf" 
        Title="MashAttack Tester" Height="720" Width="1000">
    <Grid>
        <Frame x:Name="configFrame_Copy" Content="Player &amp; Mode" HorizontalAlignment="Left" Height="88" Margin="7,73,0,0" VerticalAlignment="Top" Width="344" BorderBrush="#FF727272" BorderThickness="3" FontSize="20"/>
        <Frame x:Name="configFrame" Content="Controller Configuration" HorizontalAlignment="Left" Height="88" Margin="7,173,0,0" VerticalAlignment="Top" Width="344" BorderBrush="#FF727272" BorderThickness="3" FontSize="20"/>
        <oxy:PlotView x:Name="chart" Margin="10,338,10,24"/>
        <Button x:Name="startButton" Content="Start" Margin="0,266,641,0" VerticalAlignment="Top" Tag="startButton" Height="38" Click="startButton_Click" HorizontalAlignment="Right" Width="341" FontSize="24" FontWeight="Bold"/>
        <Grid Height="212" Margin="573,77,0,0" VerticalAlignment="Top" ShowGridLines="True" HorizontalAlignment="Left" Width="361">
            <Grid.RowDefinitions>
                <RowDefinition Height="26*"/>
                <RowDefinition Height="36*"/>
                <RowDefinition Height="29*"/>
                <RowDefinition Height="31*"/>
                <RowDefinition Height="27*"/>
                <RowDefinition Height="36*"/>
                <RowDefinition Height="27*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="111*"/>
                <ColumnDefinition Width="111*"/>
                <ColumnDefinition Width="139*"/>
            </Grid.ColumnDefinitions>
            <Label Content="Mashes" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top"/>
            <Label Content="Average Rate" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Grid.Row="1" RenderTransformOrigin="-0.023,-0.936"/>
            <Label Content="Average Up" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" Grid.Row="2"/>
            <Label Content="Average Down" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Grid.Row="3"/>
            <Label Content="Min Rate" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Grid.Row="4"/>
            <Label Content="Max Rate" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Grid.Row="5"/>
            <Label Content="Median Rate" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Grid.Row="6"/>
            <Label Content="Hertz" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1" Margin="12,0,0,0" Tag=""/>
            <Label x:Name="avgHz" Content="Average Rate" HorizontalAlignment="Left" Margin="12,5,0,0" VerticalAlignment="Top" Grid.Row="1" RenderTransformOrigin="-0.023,-0.936" Grid.Column="1" Tag="avgHz"/>
            <Label x:Name="avgUp" Content="Average Up" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" Grid.Row="2" Grid.Column="1" Tag="avgUp"/>
            <Label x:Name="avgDown" Content="Average Down" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="3" Grid.Column="1" Margin="10,0,0,0" Tag="avgDown"/>
            <Label x:Name="minRate" Content="Min Rate" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="1" Margin="12,0,0,0" Tag="minHz"/>
            <Label x:Name="maxRate" Content="Max Rate" HorizontalAlignment="Left" Margin="12,4,0,0" VerticalAlignment="Top" Grid.Row="5" Grid.Column="1" Tag="maxHz"/>
            <Label x:Name="medRate" Content="Median Rate" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="6" Grid.Column="1" Margin="12,0,0,0" Tag="medHz"/>
            <Label Content="Time" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="2" Margin="10,0,0,0"/>
            <Label x:Name="avgTime" Content="Average Rate" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Grid.Row="1" RenderTransformOrigin="-0.023,-0.936" Grid.Column="2" Tag="avgTime"/>
            <Label x:Name="minTime" Content="Min Rate" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="4" Grid.Column="2" Margin="10,0,0,0" Tag="minTime"/>
            <Label x:Name="maxTime" Content="Max Rate" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Grid.Row="5" Grid.Column="2" Tag="maxTime"/>
            <Label x:Name="medTime" Content="Median Rate" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="6" Grid.Column="2" Margin="10,0,0,0" Tag="medTime"/>
            <Label x:Name="avgUp2" Content="" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" Grid.Row="2" Grid.Column="2" Tag="avgUp" Width="80"/>
            <Label x:Name="avgDown2" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="3" Grid.Column="2" Tag="avgDown" Margin="10,0,0,0" Width="80"/>
        </Grid>
        <Rectangle x:Name="statusBar" Height="15" Margin="8,318,12,0" VerticalAlignment="Top" Stroke="Red" Fill="Red" Tag="statusBar"/>
        <Label x:Name="mashLabel" Content="Mashes" Margin="0,338,684,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="300" FontSize="24"/>
        <Label x:Name="timerLabel" Content=" " Margin="295,426,294,117" FontSize="96" HorizontalContentAlignment="Center"/>
        <ComboBox x:Name="configBox" HorizontalAlignment="Left" Height="38" Margin="196,109,0,0" VerticalAlignment="Top" Width="133"/>
        <TextBox x:Name="statusBox" HorizontalAlignment="Left" Height="205" Margin="372,77,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" IsReadOnly="True" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"/>
        <Label x:Name="scoreLabel" Content="Score" HorizontalAlignment="Right" Margin="0,338,12,0" VerticalAlignment="Top" FontSize="24" Width="300" HorizontalContentAlignment="Right"/>
        <StatusBar Margin="0,645,0,0">
            <StatusBarItem x:Name="statusLine" Content=" " Height="30" VerticalAlignment="Top" Width="976" FontSize="16" Margin="0,4,0,0"/>
        </StatusBar>
        <Menu x:Name="menuBar" Margin="0,0,0,650">
            <MenuItem x:Name="fileItem" Header="File" FontSize="20"/>
            <MenuItem x:Name="comItems" Header="COM" FontSize="20">
                <MenuItem x:Name="noneItem" Header="None" HorizontalAlignment="Left" Width="176" IsCheckable="True" IsChecked="True"/>
            </MenuItem>
            <MenuItem x:Name="updateItem" Header="Update" FontSize="20">
                <MenuItem x:Name="playerUpdate" Header="Player List" HorizontalAlignment="Left" Width="176" Click="playerUpdate_Click"/>
                <MenuItem x:Name="comUpdate" Header="COM List" HorizontalAlignment="Left" Width="176" Click="comUpdate_Click"/>
            </MenuItem>
        </Menu>
        <ComboBox x:Name="usersBox" HorizontalAlignment="Left" Margin="19,109,0,0" VerticalAlignment="Top" Width="133" Height="35"/>
        <ComboBox x:Name="inputBox" HorizontalAlignment="Left" Height="38" Margin="19,213,0,0" VerticalAlignment="Top" Width="133"/>
        <ComboBox x:Name="buttonsBox" HorizontalAlignment="Left" Height="38" Margin="195,213,0,0" VerticalAlignment="Top" Width="133"/>
        <Frame x:Name="configFrame_Copy1" Content="Statistics" HorizontalAlignment="Left" Height="250" Margin="557,54,0,0" VerticalAlignment="Top" Width="398" BorderBrush="#FF727272" BorderThickness="3" FontSize="20"/>
    </Grid>
</Window>
